//- status.jade
extends ./layout.jade
head
  block title
    title Counting Tweets
  block head
    <!--[if lte IE 8]><script language="javascript" type="text/javascript" src="/js/flot/excanvas.min.js"></script><![endif]-->
    <script type="text/javascript" src="/js/flot/jquery.flot.js"></script>
    <script type="text/javascript" src="/js/flot/jquery.flot.time.js"></script>
    <script type="text/javascript" src="/js/flot/jquery.flot.selection.js"></script>

body
  block row-content
    div.row
      div.col-lg-12
        h2(style="text-align:center") Tweets Evolution
        div.flot-placeholder#flot-placeholder1(style="width:75%;height:300px;margin:0px auto")
        div.flot-placeholder#flot-overview1(style="width:75%;height:50px;margin:0px auto")
        script.
          function getValues() {
            return !{values};
          }
        script(src="js/timestampView.js")
